describe("MainController", function() {
	it("counts the number of characters in input box", function() {
		expect(MainController.characterCount().toEqual(0))
	});
	
	it("counts the number of characters in input box with some content", function() {
		var inputStringLength = target.val().length;
		expect(MainController.characterCount().toEqual(inputStringLength))
	});
})
